{
    "name": "greencko/greencko",
    "description": "Enhanced Drupal distribution based upon Varbase",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "JÃ¶rg Riemenschneider",
            "email": "info@joergriemenschneider.de",
            "homepage": "https://www.joergriemenschneider.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/metalbote/greencko/issues",
        "source": "https://github.com/metalbote/greencko/"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "composer-patches": {
            "type": "vcs",
            "url": "https://github.com/metalbote/composer-patches.git"
        },
        "phingdrushtask": {
            "type": "vcs",
            "url": "https://github.com/pfrenssen/phingdrushtask.git"
        },
        "antennaio/jquery-bar-rating": {
            "type": "package",
            "package": {
                "name": "antennaio/jquery-bar-rating",
                "version": "1.2.2",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/antennaio/jquery-bar-rating/archive/v1.2.2.zip"
                }
            }
        },
        "greencko/greencko": {
            "type": "vcs",
            "url": "ssh://git@gitea.psh:22/Greencko/greencko.git"
        }
    },
    "require": {
        "ext-json": "*",
        "composer/installers": "^1.5",
        "cweagans/composer-patches": "~1.0",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/console": "^1.8",
        "drupal/drupal-library-installer-plugin": "^0.3",
        "drush/drush": "^9.0",
        "greencko/greencko_core": "dev-develop",
        "greencko/greencko_pages": "dev-develop",
        "greencko/greencko_toolbar": "dev-develop",
        "greencko/greencko_ui": "dev-develop",
        "oomphinc/composer-installers-extender": "^1.1",
        "vardot/varbase": "^8.6.9",
        "vardot/varbase-updater": "~1.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-core-strict": "^8.6",
        "webflo/drupal-finder": "^1.1",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-libxml": "*",
        "behat/behat": "^3.5",
        "behat/mink": "1.7.x-dev",
        "behat/mink-extension": "^2.3",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-zombie-driver": "^1.4",
        "behat/mink-selenium2-driver": "1.3.x-dev",
        "behatch/contexts": "^3.2",
        "drupal/drupal-extension": "^3.4",
        "emuse/behat-html-formatter": "dev-master",
        "rdx/behat-variables": "^1.2",
        "drupal/coder": "^8.3",
        "drupal/phingdrushtask": "dev-7.x-2.x#f4b020f09da3c174fce40f2bd9a9617689f57124",
        "drupol/phingbehattask": "^1.2",
        "geerlingguy/drupal-vm": "^5.0",
        "phing/phing": "~2.0",
        "mglaman/phpstan-drupal": "^0.11.7",
        "phpmd/phpmd": "^2.6"
    },
    "config": {
        "bin-dir": "private/bin",
        "secure-http": false,
        "optimize-autoloader": true,
        "vendor-dir": "private/vendor",
        "process-timeout": 0,
        "preferred-install": {
            "drupal/core": "dist"
        }
    },
    "autoload": {
        "psr-4": {
            "Greencko\\": "src",
            "Greencko\\Composer\\": "src/Composer",
            "Greencko\\Phing\\": "src/Phing",
            "Greencko\\tests\\": "tests/src"
        }
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "Greencko\\Composer\\ScriptHandler::createRequiredFiles",
            "@composer drupal-scaffold"
        ],
        "post-update-cmd": [
            "Greencko\\Composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-drupal-scaffold-cmd": [
            "Greencko\\Composer\\ScriptHandler::postDrupalScaffoldProcedure",
            "Greencko\\Composer\\ScriptHandler::postDrupalScaffoldSubProfileProcedure"
        ],
        "clean-dev": [
            "Greencko\\Composer\\ScriptHandler::removeGitDirectories"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "_readme": [
            "NOTICE: We're now using composer patches from Vardot repository to suggest",
            "several fixes and better handling of patches in your Drupal project.",
            "You'll notice that we have included (https://github.com/vardot/composer-patches)",
            "in this composer.json repositories. This will replace the original",
            "library (cweagans/composer-patches) with our own from (vardot/composer-patches).",
            "See https://github.com/cweagans/composer-patches/pull/243 and more details",
            "on our changes on the composer-patches package. Once our changes get merged,",
            "we will revert to using (cweagans/composer-patches) without this override."
        ],
        "branch-alias": {
            "dev-8.x-6.x": "8.6.x-dev"
        },
        "installer-types": [
            "bower-asset",
            "component",
            "library",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core",
                "drupal/core"
            ],
            "web/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "web/profiles/greencko/modules/{$name}": [
                "greencko/greencko_blog",
                "greencko/greencko_core",
                "greencko/greencko_dashboard",
                "greencko/greencko_media",
                "greencko/greencko_pages",
                "greencko/greencko_site",
                "greencko/greencko_toolbar",
                "greencko/greencko_user",
                "greencko/greencko_ui",
                "greencko/greencko_wysiwyg"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/greencko/themes/{$name}": [
                "greencko/greencko_ui"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/libraries/fontawesome": [
                "fortawesome/font-awesome"
            ],
            "web/libraries/slick": [
                "npm-asset/slick-carousel"
            ],
            "web/libraries/ace": [
                "npm-asset/ace-builds"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "private/drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-libraries": {
            "library-directory": "web/libraries",
            "libraries": [
                {
                    "name": "dropzone",
                    "package": "npm-asset/dropzone"
                },
                {
                    "name": "blazy",
                    "package": "npm-asset/blazy"
                },
                {
                    "name": "slick",
                    "package": "npm-asset/slick-carousel"
                },
                {
                    "name": "ace",
                    "package": "npm-asset/ace-builds"
                },
                {
                    "name": "spectrum",
                    "package": "npm-asset/spectrum"
                },
                {
                    "name": "fontawesome",
                    "package": "npm-asset/fortawesome--fontawesome-free"
                }
            ]
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/druadmin_lte_theme": {
                "Issue #2995449: admin_toolbar dropdown gets behind the header": "https://www.drupal.org/files/issues/2018-08-27/admin_toolbar_dropdown_gets_behind_the_header-2995449-5.patch"
            }
        },
        "patches-ignore": {
            "drupal/varbase_core": {
                "drupal/core": {
                    "Issue #2815221: Add quickedit to the latest-revision route": "https://www.drupal.org/files/issues/2018-09-24/2815221-109.patch"
                }
            },
            "greencko/greencko": {
                "drupal/bootstrap_barrio": {
                    "Issue #3033766: Notice: Array to string conversion in Drupal\\Core\\Template\\AttributeArray->__toString()": "https://www.drupal.org/files/issues/2019-02-23/bootstrap_barrio-handle_class_array-3033766-8.patch"
                },
                "drupal/bootstrap": {
                    "Issue #3027569 by markcarver, just_like_good_vibes: Error:\n Unsupported operand types during providers JSON discovery": "https://www.drupal.org/files/issues/2019-01-24/3027569-8.x-3.x-8.patch"
                }
            }
        }
    }
}
